<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.examplecclass.teaaestate.controllers.LoginController">
    <TextField fx:id="usernameField" layoutX="200" layoutY="150" promptText="Username" />
    <PasswordField fx:id="passwordField" layoutX="200" layoutY="200" promptText="Password" />
    <Button layoutX="200" layoutY="250" onAction="#login" text="Login" />
    <Button layoutX="300" layoutY="250" onAction="#switchToRegister" text="Register" />
    
    <Label fx:id="errorLabel" layoutX="200" layoutY="300" textFill="RED" />
   <Button layoutX="223.0" layoutY="339.0" mnemonicParsing="false" onAction="#goToMainMenuButton" text="Go Back to Main menu" />
</AnchorPane>
