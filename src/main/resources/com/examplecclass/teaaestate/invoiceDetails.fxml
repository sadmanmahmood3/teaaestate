<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="595.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.examplecclass.teaaestate.InvoiceDetailsController">
   <children>
      <HBox prefHeight="100.0" prefWidth="600.0" style="-fx-background-color: green;">
         <children>
            <Label alignment="CENTER" prefHeight="103.0" prefWidth="600.0" text="Invoice Details" textFill="#f2ecec">
               <font>
                  <Font name="System Bold" size="31.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Label layoutX="20.0" layoutY="122.0" text="Product Details :">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <TextField fx:id="productDetailsTextField" layoutX="145.0" layoutY="120.0" prefHeight="26.0" prefWidth="450.0" />
      <Label layoutX="20.0" layoutY="183.0" text="Payment :">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <TextField fx:id="paymenttextField" layoutX="145.0" layoutY="182.0" prefHeight="26.0" prefWidth="232.0" />
      <Label layoutX="20.0" layoutY="242.0" text="Payment Method :">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <ComboBox layoutX="162.0" layoutY="240.0" onAction="#paymentMethodOnAction" prefHeight="26.0" prefWidth="210.0" />
      <Button layoutX="414.0" layoutY="335.0" mnemonicParsing="false" onAction="#downloadInvoiceOnAction" prefHeight="31.0" prefWidth="161.0" text="Download Invoice">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
